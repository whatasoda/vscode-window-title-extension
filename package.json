{
  "name": "vscode-window-title-extension",
  "displayName": "Window Title Extension",
  "description": "Customize VSCode window title with git worktree information",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": ["Other"],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "Window Title Extension",
      "properties": {
        "windowTitleExtension.template": {
          "type": "string",
          "default": "${activeEditorShort}${separator}${rootName}${separator}${worktreeName}",
          "description": "Template for window title. Available variables: ${worktreeName}"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "bun run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "bun run compile && bun run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "typescript": "^4.9.3"
  }
}
